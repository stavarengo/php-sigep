{
    "name": "avlima/php-sigep",
    "description": "A fork from a stavarengo/php-sigep package - Integração com Web Service do Correios. Gera etiquetas, consulta preços e prazos, imprime etiquetas e PLP, etc.",
    "keywords": [
        "correios",
        "phpsigep",
        "rastreamento",
        "php-sigep",
        "sigep",
        "consulta-cep"
    ],
    "version": "0.0.1",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.4",
        "ext-soap": "*",
        "stavarengo/php-sigep-fpdf": "dev-master",
        "vria/nodiacritic": "^0.1.2"
    },
    "autoload": {
        "psr-0": {
            "PhpSigep": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Stavarengo",
            "email": "faelsta@gmail.com"
        }
    ]
}
